                        </div>
                    </div>
                    <div class="row">
                        <div class="col">

                            <div id="faq-accordion"></div>

                        </div>
                    </div>

                </section>
                <!-- course FAQ area start -->
            </div>

         
            <!-- sidebar area start -->
            <div class="col-md-4">
                <section class="course-view-sidebar-area">

                    <div class="row">
                        <div class="col">
                            <div id="fixedDiv" class="sidebar-cta-card shadow buy-card-fix">
